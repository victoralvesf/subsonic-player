{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": [
      "app",
      "dmg",
      "msi",
      "nsis"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "Aonsoku",
  "version": "0.0.0",
  "identifier": "com.victoralvesf.aonsoku",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Aonsoku",
        "label": "main",
        "width": 1280,
        "minWidth": 1280,
        "height": 720,
        "minHeight": 720,
        "maximized": true
      }
    ]
  }
}